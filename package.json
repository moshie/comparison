{
  "name": "site-shutter",
  "version": "1.1.1",
  "description": "Shutter captures screenshots of internal website pages and compares them for differences",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --opts mocha.opts && rm -rf test",
    "test:watch": "mocha --opts mocha.opts --watch",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "prepublish": "npm run compile",
    "start": "node dist/index.js"
  },
  "keywords": [
    "screenshot",
    "compare",
    "comparison"
  ],
  "bin": {
    "shutter": "./dist/index.js"
  },
  "preferGlobal": true,
  "author": "David Hewitt <hello@wp-unite.io>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "@types/mock-fs": "^3.6.30",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "mock-fs": "^4.1.0",
    "ts-node": "^2.1.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/chalk": "^0.4.31",
    "@types/commander": "^2.3.31",
    "@types/node": "^7.0.12",
    "blink-diff": "^1.0.13",
    "bluebird": "^3.4.7",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "node-spider": "^1.4.1",
    "phantomjs": "^2.1.7",
    "typescript": "^2.2.1"
  }
}
